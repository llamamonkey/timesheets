{
    "rules": {
        "users": {
          ".read": false,
          ".write": "auth != null && !data.child(auth.uid).exists()",
            "$user_id": {
                ".read": "auth.uid === $user_id",
                ".write": "auth.uid === $user_id"
            }
        },
        "time": {
            "$user_id": {
                ".read": "auth.uid === $user_id",
                ".write": "auth.uid === $user_id",
                "$time_date": {
                    //Validation rules go here
                }
            }          
        }
    }
}